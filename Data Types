#include<iostream>
using namespace std;
int main(){
    int age=25;
    cout<<age<<endl;
    char grade = 'a';
    cout<<grade<<endl;
    float PI = 3.14f;
    cout<<PI<<endl;
    bool isSafe=true;
    cout<<isSafe<<endl;
    double price = 100.99;
    cout<<price<<endl;
    return 0;
}
Output: 
25
a
3.14
1
100.99
